package patterns;
import java.util.*;

public class learning {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n =sc.nextInt();
		for(int row =0; row<n; row++) {
			System.out.println( );
			for(int j=0; j<row; j++) {
				System.out.println("*");
			}
		}
	}

}
