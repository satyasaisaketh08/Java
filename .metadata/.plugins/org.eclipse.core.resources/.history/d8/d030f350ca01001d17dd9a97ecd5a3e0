package linked_lists;

public class demo {

	class Node {
		public Node(int ali) {
			// TODO Auto-generated constructor stub
			data = ali;
		}

		int data;
		Node next;
	}

	Node head;
	
	public void disp() {
		Node temp = head;
		while (temp != null) {
			System.out.print(temp.data+" ");
			temp = temp.next;
		}
		System.out.println();
	}
	
	public int size() {
		Node temp = head;
		int size = 0;
		while (temp != null) {
//			System.out.print(temp.data+" ");
			temp = temp.next;
			size++;
		}
		size++;
		
	}
	
}
