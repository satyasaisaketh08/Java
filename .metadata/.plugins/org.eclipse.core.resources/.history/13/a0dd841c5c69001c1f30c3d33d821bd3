package tute;
import java.util.*;

public class prime {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc = new Scanner(System.in);
		int number = sc.nextInt();
		int i = 1;
		int count_factor = 0;
		
		while(i<=number){
			 if (number % i == 0) {
			        count_factor += 1;
			        
			      }
			 i++;
		}
		
		if (count_factor<=2) {
			System.out.println("prime number");
		}else {
			System.out.println("not prime");
		}
		
	}

}
