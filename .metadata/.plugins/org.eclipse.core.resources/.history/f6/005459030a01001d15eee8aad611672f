package queue;

import java.util.*;

public class first_neg_num {
	
	// Constructor class
	
	static Queue <Integer> Q;

	public first_neg_num() {
		Q = new LinkedList<>();
	}
	
	public static void solve(int[] arr , int k) {
		for(int i=;i<k;i++) {
			if(arr[i]  <0) {
				Q.add(i);
			}
		}
	}
	
	public static void main(String[] args) {
		
	}
	
	

}
